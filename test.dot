graph {      
    node [shape=ellipse];          

    // declare nodes
    a b c d e f;
     
    // declare edges
    a -- b; 
    a -- f; 
    a -- c; 
    a -- d; 
    a -- e; 
    b -- c; 
    b -- d; 
    b -- e; 
    b -- f; 
    c -- d; 
    c -- e; 
    c -- f; 
    d -- e; 
    d -- f; 
    e -- f; 

    // scale up output to avoid overlap
    overlap = scale;
}
